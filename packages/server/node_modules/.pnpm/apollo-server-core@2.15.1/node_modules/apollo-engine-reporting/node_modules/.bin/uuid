#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/uuid@8.2.0/node_modules/uuid/dist/bin/uuid/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/uuid@8.2.0/node_modules/uuid/dist/bin/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/uuid@8.2.0/node_modules/uuid/dist/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/uuid@8.2.0/node_modules/uuid/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/uuid@8.2.0/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules:/mnt/c/www/gama/graphql/packages/node_modules:/mnt/c/www/gama/graphql/node_modules:/mnt/c/www/gama/node_modules:/mnt/c/www/node_modules:/mnt/c/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/apollo-engine-reporting@2.2.1/node_modules/uuid/dist/bin/uuid/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/apollo-engine-reporting@2.2.1/node_modules/uuid/dist/bin/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/apollo-engine-reporting@2.2.1/node_modules/uuid/dist/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/apollo-engine-reporting@2.2.1/node_modules/uuid/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/apollo-engine-reporting@2.2.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../uuid/dist/bin/uuid" "$@"
else 
  exec node  "$basedir/../../../uuid/dist/bin/uuid" "$@"
fi
