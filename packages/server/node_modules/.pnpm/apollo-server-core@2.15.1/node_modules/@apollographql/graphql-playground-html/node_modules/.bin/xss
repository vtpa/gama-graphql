#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/xss@1.0.7/node_modules/xss/bin/xss/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/xss@1.0.7/node_modules/xss/bin/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/xss@1.0.7/node_modules/xss/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/xss@1.0.7/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules:/mnt/c/www/gama/graphql/packages/node_modules:/mnt/c/www/gama/graphql/node_modules:/mnt/c/www/gama/node_modules:/mnt/c/www/node_modules:/mnt/c/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/@apollographql/graphql-playground-html@1.6.26/node_modules/xss/bin/xss/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/@apollographql/graphql-playground-html@1.6.26/node_modules/xss/bin/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/@apollographql/graphql-playground-html@1.6.26/node_modules/xss/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/@apollographql/graphql-playground-html@1.6.26/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/@apollographql/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../xss/bin/xss" "$@"
else 
  exec node  "$basedir/../../../../xss/bin/xss" "$@"
fi
