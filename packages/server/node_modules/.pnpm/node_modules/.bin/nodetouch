#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/nodetouch.js/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/touch@3.1.0/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules:/mnt/c/www/gama/graphql/packages/node_modules:/mnt/c/www/gama/graphql/node_modules:/mnt/c/www/gama/node_modules:/mnt/c/www/node_modules:/mnt/c/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules/touch/bin/nodetouch.js/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules/touch/bin/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules/touch/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else 
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
