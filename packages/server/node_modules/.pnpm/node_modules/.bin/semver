#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/semver/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/semver@5.7.1/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules:/mnt/c/www/gama/graphql/packages/node_modules:/mnt/c/www/gama/graphql/node_modules:/mnt/c/www/gama/node_modules:/mnt/c/www/node_modules:/mnt/c/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules/semver/bin/semver/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules/semver/bin/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules/semver/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver" "$@"
else 
  exec node  "$basedir/../semver/bin/semver" "$@"
fi
