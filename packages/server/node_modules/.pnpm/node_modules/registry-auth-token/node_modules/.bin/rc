#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/rc@1.2.8/node_modules/rc/cli.js/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/rc@1.2.8/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules:/mnt/c/www/gama/graphql/packages/node_modules:/mnt/c/www/gama/graphql/node_modules:/mnt/c/www/gama/node_modules:/mnt/c/www/node_modules:/mnt/c/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/registry-auth-token@4.2.0/node_modules/rc/cli.js/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/registry-auth-token@4.2.0/node_modules/rc/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/registry-auth-token@4.2.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../rc/cli.js" "$@"
else 
  exec node  "$basedir/../../../rc/cli.js" "$@"
fi
