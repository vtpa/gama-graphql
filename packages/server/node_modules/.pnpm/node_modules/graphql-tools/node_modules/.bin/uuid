#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/uuid/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/uuid@3.4.0/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules:/mnt/c/www/gama/graphql/packages/node_modules:/mnt/c/www/gama/graphql/node_modules:/mnt/c/www/gama/node_modules:/mnt/c/www/node_modules:/mnt/c/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/graphql-tools@4.0.8/node_modules/uuid/bin/uuid/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/graphql-tools@4.0.8/node_modules/uuid/bin/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/graphql-tools@4.0.8/node_modules/uuid/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/graphql-tools@4.0.8/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../uuid/bin/uuid" "$@"
else 
  exec node  "$basedir/../../../uuid/bin/uuid" "$@"
fi
