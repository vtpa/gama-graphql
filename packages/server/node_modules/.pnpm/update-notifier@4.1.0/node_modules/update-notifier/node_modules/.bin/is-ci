#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/bin.js/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/is-ci@2.0.0/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules:/mnt/c/www/gama/graphql/packages/node_modules:/mnt/c/www/gama/graphql/node_modules:/mnt/c/www/gama/node_modules:/mnt/c/www/node_modules:/mnt/c/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/update-notifier@4.1.0/node_modules/is-ci/bin.js/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/update-notifier@4.1.0/node_modules/is-ci/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/update-notifier@4.1.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../is-ci/bin.js" "$@"
else 
  exec node  "$basedir/../../../is-ci/bin.js" "$@"
fi
