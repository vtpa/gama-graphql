#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/@apollo/protobufjs@1.0.4/node_modules/@apollo/protobufjs/bin/pbjs/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/@apollo/protobufjs@1.0.4/node_modules/@apollo/protobufjs/bin/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/@apollo/protobufjs@1.0.4/node_modules/@apollo/protobufjs/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/@apollo/protobufjs@1.0.4/node_modules/@apollo/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/@apollo/protobufjs@1.0.4/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/@apollo/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules:/mnt/c/www/gama/graphql/packages/node_modules:/mnt/c/www/gama/graphql/node_modules:/mnt/c/www/gama/node_modules:/mnt/c/www/node_modules:/mnt/c/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/apollo-engine-reporting-protobuf@0.5.2/node_modules/@apollo/protobufjs/bin/pbjs/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/apollo-engine-reporting-protobuf@0.5.2/node_modules/@apollo/protobufjs/bin/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/apollo-engine-reporting-protobuf@0.5.2/node_modules/@apollo/protobufjs/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/apollo-engine-reporting-protobuf@0.5.2/node_modules/@apollo/node_modules:/mnt/c/www/gama/graphql/packages/server/node_modules/.pnpm/apollo-engine-reporting-protobuf@0.5.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../@apollo/protobufjs/bin/pbjs" "$@"
else 
  exec node  "$basedir/../../../@apollo/protobufjs/bin/pbjs" "$@"
fi
